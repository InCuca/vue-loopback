{
  "root": true,
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "env": {
    "browser": true
  },
  "extends": [
    "plugin:vue/recommended",
    "airbnb-base",
    "loopback"
  ],
  "plugins": [
    "vue"
  ],
  "settings": {
    "import/resolver": "./.resolver.js"
  },
  "globals": {
    "expect": true,
    "assert": true,
    "require": true,
    "request": true
  }
}
