{
  "presets": [
    ["env", {
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8"
          ]
        }
      }],
    "stage-2"
  ],
  "plugins": [
    ["babel-plugin-root-import", [
        {
          "rootPathPrefix": "~",
          "rootPathSuffix": "."
        },
        {
          "rootPathPrefix": "@",
          "rootPathSuffix": "client"
        }
    ]]
  ]
}
