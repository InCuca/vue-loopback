{
  "presets": [
    ["env", {
        "targets": {
          "browsers": [
            "> 1%",
            "last 2 versions",
            "not ie <= 8"
          ],
          "node": "8"
        }
      }]
  ],
  "plugins": [
    [
      "babel-plugin-root-import",
      [
        {
          "rootPathPrefix": "~",
          "rootPathSuffix": "."
        },
        {
          "rootPathPrefix": "@",
          "rootPathSuffix": "client"
        }
      ]
    ],
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ]
  ]
}
