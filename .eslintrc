    },
    "env": {
        "browser": true
    },
    "extends": [
        "plugin:vue/recommended",
        "airbnb-base",
        "loopback"
    ],
    "plugins": [
        "vue"
    ],
    "globals": {
        "expect": true,
        "assert": true,
        "require": true,
        "request": true
    }
}
